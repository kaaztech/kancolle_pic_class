<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>kancolle shiplist collect</string>
        <key>ProgramArguments</key>
        <array>
            <string>sh</string>
            <string>/Users/hogeuser/cron/shiplist.sh</string>
        </array>
        <key>LowPriorityIO</key>
        <true/>
        <key>Nice</key>
        <integer>1</integer>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Hour</key>
            <integer>23</integer>
            <key>Minute</key>
            <integer>30</integer>
        </dict>
	<!-- ログ指定 -->
	<key>StandardErrorPath</key>
	<string>/Users/hogeuser/cron/log/err.log</string>
	<key>StandardOutPath</key>
	<string>/Users/hogeuser/cron/log/log.log</string>
	<!-- 処理時間の制限 -->
	<key>ExitTimeOut</key>
	<integer>1200</integer>
    </dict>
</plist>
